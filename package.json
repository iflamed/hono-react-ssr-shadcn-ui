{
  "name": "hono-react-ssr",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "echo \"export default false\" > ./src/config/is_prod.ts && vite",
    "client": "vite --mode client",
    "build": "$npm_execpath run version && vite build --mode client && cp -f ./dist/.vite/manifest.json ./src/lib/manifest.json && vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "$npm_execpath run build && wrangler pages deploy dist",
    "version": "echo \"export default `date +'%s'`\" > ./src/config/build_time.ts && echo \"export default true\" > ./src/config/is_prod.ts"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.1",
    "@hono/react-renderer": "^1.0.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.0",
    "@sindresorhus/slugify": "^2.2.1",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "hono": "^4.9.4",
    "i18next": "^25.4.0",
    "iso-639-1": "^3.1.5",
    "lucide-react": "^0.541.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.1",
    "react-share": "^5.2.2",
    "remarkable": "^2.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250415.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "@hono/vite-dev-server": "^0.20.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/remarkable": "^2.0.8",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "rollup-preserve-directives": "^1.1.1",
    "tailwindcss": "^4.1.11",
    "vite": "^7.1.1",
    "wrangler": "^4.28.1"
  }
}
